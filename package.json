{
  "name": "product.io",
  "version": "0.0.1",
  "description": "BWB app",
  "productName": "bwb",
  "cordovaId": "org.cordova.bwb.app",
  "author": "Rodrigo Alves <igo.sud@hotmail.com>",
  "private": true,
  "scripts": {
    "pwa": "quasar dev -m pwa",
    "dev": "quasar dev",
    "dev:ios": "quasar dev -m cordova -T ios",
    "dev:android": "quasar dev -m cordova -T android",
    "deploy": "quasar build -m pwa && cp -r dist/pwa/* public/ && firebase deploy",
    "start": "npm run dev",
    "start:pwa": "npm run pwa"
  },
  "dependencies": {
    "@quasar/extras": "^1.0.0",
    "axios": "^0.18.0",
    "quasar": "^1.0.0-beta.0",
    "vue-i18n": "^7.3.3"
  },
  "devDependencies": {
    "@quasar/app": "^1.0.0-beta.0",
    "strip-ansi": "=3.0.1"
  },
  "engines": {
    "node": ">= 8.9.0",
    "npm": ">= 5.6.0",
    "yarn": ">= 1.6.0"
  },
  "browserslist": [
    "last 1 version, not dead, ie >= 11"
  ],
  "resolutions": {
    "ajv": "6.8.1"
  }
}